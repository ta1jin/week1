{
  "routes": {
    "/login/*": "index.html",
    "/sample/*": "index.html",
    "/promise/*": "index.html",
    "/fetch/*": "index.html"
  },
  "headers": {
    "/login/*": {
      "Content-Type": "text/plain; charset=UTF-8",
      "Access-Control-Allow-Origin": "*"
    },
    "/sample/*": {
      "Content-Type": "text/plain; charset=UTF-8",
      "Access-Control-Allow-Origin": "*"
    },
    "/promise/*": {
      "Content-Type": "text/plain; charset=UTF-8",
      "Access-Control-Allow-Origin": "*"
    },
    "/fetch/*": {
      "Access-Control-Allow-Origin": "*"
    }
  }
}
